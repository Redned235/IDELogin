{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "A helper mod for logging in to a Minecraft account while testing in the IDE.",
  "authors": [
    "${author}"
  ],

  "license": "CC0-1.0",
  "icon": "assets/idelogin/logo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.redned.idelogin.fabric.IdeLoginFabric"
    ]
  },
  "mixins": [
    "idelogin.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=21"
  }
}
